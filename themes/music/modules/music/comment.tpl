<!-- BEGIN: main --><!-- BEGIN: prev --><a class="musicicon mprev" href="javascript:void(0);" title="{LANG.prev}" onclick="show_comment('{GDATA.id}', '{GDATA.where}', {prev});">{LANG.prev}</a><!-- END: prev --><!-- BEGIN: next --><a class="musicicon mnext" href="javascript:void(0);" title="{LANG.next}" onclick="show_comment('{GDATA.id}', '{GDATA.where}', {next});">{LANG.next}</a><!-- END: next --><!-- BEGIN: datacomment --><table cellpadding="0" cellspacing="0" class="musictable mtcomment">	<!-- BEGIN: loop -->	<tr>		<td class="comment-left"><img class="musicsmalllalbum" alt="{ROW.name}" src="{ROW.avatar}" width="50" height="50"/></td>		<td>			<strong class="greencolor">{ROW.name}</strong> - <span class="msmall">{ROW.date}</span><br />			{ROW.body}				</td>	</tr><!-- END: loop --></table><!-- END: datacomment --><!-- END: main -->